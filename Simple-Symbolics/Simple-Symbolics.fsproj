<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net45;netstandard2.0;netcoreapp2.0</TargetFrameworks>
    <Authors>Deen Abiola</Authors>
    <Version>0.1.1</Version>
    <Description>Provides functionality over bare mathnet-symbolics, including basic integration, linear algebra, solving and other interesting capabilities (such as combining with probabilistic programming).</Description> 
  </PropertyGroup>

  <ItemGroup>
    <None Include="Lab\linear-algebra.fsx" />
    <None Include="Lab\linalg-tests.fsx" />
    <None Include="Lab\Core.fsx" />
    <None Include="Lab\differential-calculus.fsx" />
    <None Include="Lab\integration.fsx" />
    <None Include="Lab\solving.fsx" />
    <None Include="Lab\experiments.fsx" />
    <Compile Include="Core.fs" />
    <Compile Include="LinearAlgebra.fs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="FParsec" Version="1.0.3" />
    <PackageReference Include="MathNet.Numerics.FSharp" Version="4.6.0" />
    <PackageReference Include="MathNet.Symbolics" Version="0.19.0" />
  </ItemGroup>

</Project>
